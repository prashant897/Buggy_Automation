{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "lib": [
      "ES2020",
      "DOM"
    ],
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "outDir": "dist",
    "types": [
      "node",
      "playwright"
    ]
  },
  "include": [
    "tests",
    "src",
    "playwright.config.ts"
  ]
}